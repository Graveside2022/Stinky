# Stinkster Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# === OpenWebRX Configuration ===
OPENWEBRX_ADMIN_USER=admin
OPENWEBRX_ADMIN_PASSWORD=CHANGE_ME_SECURE_PASSWORD
OPENWEBRX_TITLE=Stinkster_SDR
OPENWEBRX_LOCATION=Your_Location_Here
OPENWEBRX_DEBUG=false
OPENWEBRX_PORT=8073

# === TAK Server Configuration ===
TAK_SERVER_IP=YOUR_TAK_SERVER_IP
TAK_SERVER_PORT=8087
TAK_SERVER_PROTOCOL=tcp
TAK_CALLSIGN=YOUR_CALLSIGN
TAK_UID=YOUR_UNIQUE_ID

# === GPS Configuration ===
GPS_DEVICE=/dev/ttyUSB0
GPS_BAUDRATE=4800
GPSD_PORT=2947

# === MAVLink Configuration ===
MAVLINK_DEVICE=/dev/ttyUSB0
MAVLINK_BAUDRATE=57600
MAVPROXY_PORT=14550

# === WiFi Scanning Configuration ===
WIFI_INTERFACE=wlan2
KISMET_WEB_PORT=2501
KISMET_API_PORT=2502

# === Flask Web Services ===
WIGLETOTAK_PORT=6969
WIGLETOTAK_HOST=0.0.0.0
SPECTRUM_ANALYZER_PORT=5000
WEBHOOK_PORT=5001

# === System Paths ===
PROJECT_DIR=/home/pi/projects/stinkster
LOG_DIR=/home/pi/tmp
KISMET_DATA_DIR=/home/pi/kismet_ops
BACKUP_DIR=/home/pi/backups

# === Docker Configuration ===
DOCKER_COMPOSE_PROJECT_NAME=stinkster
DOCKER_RESTART_POLICY=unless-stopped

# === Security Settings ===
FLASK_SECRET_KEY=GENERATE_A_RANDOM_SECRET_KEY_HERE
API_KEY=YOUR_API_KEY_FOR_WEBHOOKS