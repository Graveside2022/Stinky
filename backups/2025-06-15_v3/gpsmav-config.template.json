{
  "mavlink": {
    "device": "/dev/ttyUSB0",
    "baudrate": 57600,
    "source_system": 1,
    "source_component": 1,
    "target_system": 0,
    "target_component": 0
  },
  "gpsd": {
    "host": "localhost",
    "port": 2947,
    "mode": 3,
    "device": "/dev/ttyUSB0"
  },
  "mavproxy": {
    "master": "tcp:127.0.0.1:14550",
    "out": [
      "udp:127.0.0.1:14551",
      "udp:127.0.0.1:14552"
    ],
    "modules": ["graph", "map"],
    "auto_add_modules": true
  },
  "logging": {
    "level": "INFO",
    "file": "${LOG_DIR}/gpsmav.log",
    "console": true
  },
  "retry": {
    "max_attempts": 5,
    "delay_seconds": 2
  }
}